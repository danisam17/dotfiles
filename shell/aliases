alias _a="git add"
alias _af="git commit --amend; git push -f"
alias _am="git commit --amend"
alias _c="git commit"
alias _cp="git commit; git push"
alias _cpf="git commit; git push -f"
alias _d="git diff"
alias _dc="git diff --cached"
alias _p="git push"
alias _pf="git push -f"
alias _s="scmpuff_status"
alias _x="exec $SHELL -l"
