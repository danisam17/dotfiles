for file in $HOME/.machines/*; do
  source $file
done

for file in $HOME/.shell/*; do
  source $file
done

eval "$(hub alias -s)"
eval "$(rbenv init -)"

export EDITOR=vim

### Added by the Heroku Toolbelt
export PATH="/usr/local/heroku/bin:$PATH"

source ~/.bin/z.sh

# added by travis gem
[ -f /Users/kristian/.travis/travis.sh ] && source /Users/kristian/.travis/travis.sh

