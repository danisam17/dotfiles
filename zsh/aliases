alias am="g amend"
alias back="rake db:rollback"
alias be="bundle exec"
alias cob="git checkout -b"
alias die="exec $SHELL -l"
alias g=git
alias gc="git clone"
alias gac="git add . && git commit"
alias gd="git di"
alias gdf="git difftool --no-prompt --tool=vimdiff"
alias gp="git pull --rebase"
alias gs="git st"
alias gu="git add --update :/"
alias l="tree -L 1 -a"
alias new="git checkout -b"
alias p=python
alias pm="python manage.py"
alias rcr="rake db:create"
alias rmi="rake db:migrate db:test:prepare"
alias pro="ps aux | grep"
alias site="cd ~/src/website"
alias t="tmuxinator"
alias v=vim
alias ve="vim ~/.vimrc"
alias vi=vim
alias vup="vim-update-bundles -n"
alias xx="chmod +x"
alias zef="vim ~/.zsh/functions"
alias zea="vim ~/.zsh/aliases"
alias ze="vim ~/.zshrc"
