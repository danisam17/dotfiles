alias am="g amend"
alias die="exec $SHELL -l"
alias g=git
alias gc="git clone"
alias gac="git add . && git commit"
alias gd="g di"
alias gpp="git pull --rebase; git push"
alias gs="g st"
alias gu="git add --update :/"
alias l=ls
alias less="vim -u ~/.vimrc.more"
alias ls="ls -Ga"
alias rcr="rake db:create"
alias rmi="rake db:migrate db:test:prepare"
alias v=vim
alias ve="vim ~/.vimrc"
alias vi=vim
alias zef="vim ~/.zsh/functions"
alias zea="vim ~/.zsh/aliases"
alias ze="vim ~/.zshrc"
