alias am="g amend"
alias bundle='bundle -j8'
alias die="exec $SHELL -l"
alias g=git
alias gc="git clone"
alias gac="git add . && git commit"
alias gd="git diff"
alias gdf="git difftool --no-prompt --tool=vimdiff"
alias gp="git pull --rebase"
alias gpp="git pull --rebase; git push"
alias gs="git st"
alias gu="git add --update :/"
alias l=ls
alias less="vim -u ~/.vimrc.more"
alias ls="ls -Ga"
alias rcr="rake db:create"
alias back="rake db:rollback"
alias rmi="rake db:migrate db:test:prepare"
alias pro="ps aux | grep"
alias site="cd ~/source/ruby/site"
alias v=vim
alias ve="vim ~/.vimrc"
alias vi=vim
alias zef="vim ~/.zsh/functions"
alias zea="vim ~/.zsh/aliases"
alias ze="vim ~/.zshrc"
