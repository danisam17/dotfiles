c() {
  builtin cd "$@" && ls
}

f() {
  ack $1 -k *
}

gpp() {
  if [ -z $1 ]
  then
    remote=$1
  else
    remote="origin"
  fi

  branch=$(git rev-parse --abbrev-ref HEAD)
  git pull --rebase $remote $branch
  git push $remote $branch
}

nmux() {
  current=$(echo $(basename $(pwd)))
  tmux new -s $current
}
