current() {
  printf $(basename `pwd`)
}

attach() {
  tmux at -t $(current)
}

f() {
  ack $1 -k *
}

gpp() {
  if [ -z $1 ]
  then
    remote="origin"
  else
    remote=$1
  fi

  branch=$(git rev-parse --abbrev-ref HEAD)
  git pull --rebase $remote $branch
  git push $remote $branch
}

nmux() {
  tmux new -s $(current)
}

clip() {
  cat $1 | pbcopy
}
